<% @user_addresses.each do |address| %>
  <%= form_for address do |f| %>
    <div class = "mng-address">
      <%= f.label :house_no %>:
      <%= f.text_field :house_no %><br />
        <%= f.label :street %>:
        <%= f.text_field :street %><br/>
        <%= f.label :city %>:
        <%= f.text_field :city %><br/>
        <%= f.label :state %>:
        <%= f.text_field :state %><br/>
        <%= f.label :pin %>:
        <%= f.text_field :pin %><br/>
      <%= f.submit %>
    </div><br/>
  <% end %>
<%end%>


